<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.summary.zkhdsummary.mapper.LogMapper" >
    <!--查询出所有log记录-->
       <select id="findList" resultType="com.summary.zkhdsummary.bean.Log">
           select * from log;
       </select>
    <!--根据user id查询出其发表总结的时间-->
    <select id="findTime" resultType="java.util.Date" parameterType="int" >
           select date from log where userid = #{userid};

    </select>
    <!--根据user id 查询出其发表总结-->
    <select id="findSummaryById" resultType="com.summary.zkhdsummary.bean.Log" parameterType="int" >
           select * from log where userid = #{userid};
    </select>
</mapper>